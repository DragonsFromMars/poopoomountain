{
  "version": 2,
  "functions": {
    "dist/index.js": { "runtime": "nodejs22.x" }
  },
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/dist/index.js" },
    { "handle": "filesystem" },
    { "src": "^(?!/api)(.*)$", "dest": "/dist/public$1" },
    { "src": "^(?!/api)(.*)", "dest": "/dist/public/index.html" }
  ]
}
